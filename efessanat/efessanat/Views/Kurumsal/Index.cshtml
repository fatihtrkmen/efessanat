    <!-- WRAPPER -->
    <div id="wrapper">

        <!-- PAGE TITLE -->
        <header id="page-title">
            <div class="container">
                <h1>Kurumsal</h1>

                <ul class="breadcrumb">
                    <li><a href="index.html">Anasayfa</a></li>
                    <li class="active">Kurumsal</li>
                </ul>
            </div>
        </header>

        <section class="container">

            <!-- WELCOME -->
            <article>
                @if (ViewData["KurumsalHosgeldiniz"] != null)
            {
                var item = ViewData["KurumsalHosgeldiniz"] as efessanat.Models.KurumsalHosgeldinizs;
                    @Html.Raw(item.Yazi)

                }
            </article>
            <!-- WELCOME -->

            <div class="divider">
                <!-- divider -->
                <i class="fa fa-star"></i>
            </div>

            <!-- BORN TO BE A WINNER -->
            <article class="row">
                <div class="col-md-6">
                    <div class="owl-carousel controlls-over" data-plugin-options='{"items": 1, "singleItem": true, "navigation": true, "pagination": true, "transitionStyle":"fadeUp"}'>

                        @if (ViewData["KurumsalEkipFotos"] != null)
                        {
                            foreach (var KurumsalEkipFotos in ViewData["KurumsalEkipFotos"] as IEnumerable<efessanat.Models.KurumsalEkipFotos>)
                            {
                            <div>
                                <img class="img-responsive" src="~/Assets/Fotograflar/Kurumsal/EkipFoto/@KurumsalEkipFotos.Name" width="555" height="311" alt="">
                            </div>
                            }
                        }


                    </div>
                </div>
                <div class="col-md-6">
                    @if (ViewData["KurumsalEkips"] != null)
            {
                var item = ViewData["KurumsalEkips"] as efessanat.Models.KurumsalEkips;
                        @Html.Raw(item.Yazi)

                    }
                </div>
            </article>
            <!-- /BORN TO BE A WINNER -->


            <div class="divider">
                <!-- divider -->
                <i class="fa fa-star"></i>
            </div>


            <!-- WHO WE ARE and SKILLS -->
            <article class="row">
                <div class="col-md-6">
                    <h3>Efes Sanat Kimdir..</h3>

                    <div class="toogle">

                        @if (ViewData["EfesSanatKimdir"] != null)
                        {
                            foreach (var EfesSanatKimdir in ViewData["EfesSanatKimdir"] as IEnumerable<efessanat.Models.EfesSanatKimdir>)
                            {
                                <div class="toggle">
                                    <label>@EfesSanatKimdir.Baslik</label>
                                    <div class="toggle-content">
                                        <p>@EfesSanatKimdir.Aciklama</p>
                                    </div>
                                </div>
                            }
                        }



                    </div>
                </div>
                <div class="col-md-6">
                    <h3>Uzmanlık Alanları</h3>

                    <div class="progress-bars">

                        @if (ViewData["EfesSanatYetenekler"] != null)
                        {
                            foreach (var EfesSanatYetenekler in ViewData["EfesSanatYetenekler"] as IEnumerable<efessanat.Models.EfesSanatYetenekler>)
                            {
                                <div class="progress-label">
                                    <span>@EfesSanatYetenekler.Yetenek</span>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-primary" data-appear-progress-animation="@EfesSanatYetenekler.Deger%">
                                        <span class="progress-bar-tooltip">@EfesSanatYetenekler.Deger</span>
                                    </div>
                                </div>

                            }
                        }


                      

                    </div>

                </div>
            </article>
            <!-- /WHO WE ARE and SKILLS -->


            <div class="divider">
                <!-- divider -->
                <i class="fa fa-star"></i>
            </div>

            <!-- TEAM -->
            <article class="row">
                <div class="col-md-12">
                    <h3>Ekibimiz</h3>

                    <div class="row">


                           

                                @if (ViewData["KurumsalCalisanBilgileri"] != null)
                                {
                                    foreach (var KurumsalCalisanBilgileri in ViewData["KurumsalCalisanBilgileri"] as IEnumerable<efessanat.Models.KurumsalCalisanBilgileri>)
                                    {
                                        <div class="col-sm-6 col-md-3">
                                            <!-- item 1 -->
                                            <div class="item-box fixed-box">
                                                <figure>
                                                    <img class="img-responsive" src="~/Assets/Fotograflar/Kurumsal/EkipUyeleri/@KurumsalCalisanBilgileri.Id/@KurumsalCalisanBilgileri.filename" width="263" height="263" alt="">
                                                </figure>
                                                <div class="item-box-desc">
                                                    <h4>@KurumsalCalisanBilgileri.isim</h4>
                                                    <small class="styleColor">@KurumsalCalisanBilgileri.pozisyon</small>
                                                    <div><p>@KurumsalCalisanBilgileri.detay.</p></div>
                                                    
                                                    <div class="row socials">
                                                        <a href="@KurumsalCalisanBilgileri.fb" class="social fa fa-facebook"></a>
                                                        <a href="@KurumsalCalisanBilgileri.twitter" class="social fa fa-twitter"></a>
                                                        <a href="@KurumsalCalisanBilgileri.gmail" class="social fa fa-google-plus"></a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                }

                    </div>

                </div>
            </article>
            <!-- /TEAM -->


            <hr />

            <div class="owl-carousel" data-plugin-options='{"items": 5, "singleItem": false, "autoPlay": true, "navigation": false, "pagination": false}'>
                @if (ViewData["references"] != null)
                {
                    foreach (var item in ViewData["references"] as IEnumerable<efessanat.Models.References>)
                    {
                        <div>
                            <img class="img-responsive" src="~/References/@item.Name/logo.jpg" alt="">
                        </div>
                    }
                }
            </div>

        </section>

    </div>
    <!-- /WRAPPER -->
    <!-- FOOTER -->
    <footer>

        <!-- copyright , scrollTo Top -->
        <div class="footer-bar">
            <div class="container">
                <span class="copyright">Copyright &copy; Efes Sanat. Tüm hakları saklıdır.</span>
                <a class="toTop" href="#topNav">BAŞA DÖN <i class="fa fa-arrow-circle-up"></i></a>
            </div>
        </div>
        <!-- copyright , scrollTo Top -->
        <!-- footer content -->
        <div class="footer-content">
            <div class="container">

                <div class="row">


                    <!-- FOOTER CONTACT INFO -->
                    <div class="column col-md-4">
                        @if (ViewData["AnasayfaIletisimKismi"] != null)
                        {
                            var item = ViewData["AnasayfaIletisimKismi"] as efessanat.Models.AnasayfaIletisimKismi;
                            <h3>@Html.Raw(item.Baslik)</h3>
                            <p class="contact-desc">
                                @Html.Raw(item.Yazi)
                            </p>
                            <address class="font-opensans">
                                <ul>
                                    <li class="footer-sprite address">
                                        @Html.Raw(item.Adres)
                                    </li>
                                    <li class="footer-sprite phone">
                                        Phone: @Html.Raw(item.Numara)
                                    </li>
                                    <li class="footer-sprite email">
                                        <a href="mailto:@item.Mail">@Html.Raw(item.Mail)</a>
                                    </li>
                                </ul>
                            </address>
                        }

                    </div>
                    <!-- /FOOTER CONTACT INFO -->
                    <!-- FOOTER LOGO -->
                    <div class="column logo col-md-4 text-center">
                        <div class="logo-content">
                            <img class="animate_fade_in" src="assets/images/logo_footer.png" width="200" alt="" />
                            <h4>ATROPOS TEMPLATE</h4>
                        </div>
                    </div>
                    <!-- /FOOTER LOGO -->
                    <!-- FOOTER LATEST POSTS -->
                    <div class="column col-md-4 text-right">
                        <h3>RECENT POSTS</h3>

                        <div class="post-item">
                            <small>JANUARY 2, 2014 BY ADMIN</small>
                            <h3><a href="blog-single-sidebar-left.html">Lorem ipsum dolor sit amet, consectetur adipiscing elit</a></h3>
                        </div>
                        <div class="post-item">
                            <small>JANUARY 2, 2014 BY ADMIN</small>
                            <h3><a href="blog-single-sidebar-left.html">Lorem ipsum dolor sit amet, consectetur adipiscing elit</a></h3>
                        </div>
                        <div class="post-item">
                            <small>JANUARY 2, 2014 BY ADMIN</small>
                            <h3><a href="blog-single-sidebar-left.html">Lorem ipsum dolor sit amet, consectetur adipiscing elit</a></h3>
                        </div>

                        <a href="blog-masonry-sidebar.html" class="view-more pull-right">View Blog <i class="fa fa-arrow-right"></i></a>

                    </div>
                    <!-- /FOOTER LATEST POSTS -->

                </div>

            </div>
        </div>
        <!-- footer content -->

    </footer>
